{
	"name": "jwt-decoder",
	"displayName": "jwt-decoder",
	"description": "A simple JWT decoder extension for VS Code",
    "icon": "images/icon.png",
    "publisher": "jflbr",
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.jwt-decoder"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.jwt-decoder",
				"title": "JWT Decoder"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
    },
    "repository": {
        "url": "https://github.com/jflbr/jwt-decoder.git",
        "type": "git"
    },
    "homepage": "https://github.com/jflbr/jwt-decoder/blob/master/README.MD"
}
